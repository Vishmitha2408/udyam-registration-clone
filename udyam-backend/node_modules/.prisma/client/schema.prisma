// prisma/schema.prisma

generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Submission {
  id        Int      @id @default(autoincrement())
  name      String
  aadhaar   String
  pan       String
  pin       String
  city      String
  state     String
  createdAt DateTime @default(now())
}
